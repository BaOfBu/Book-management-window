<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Flora.ViewModel"
                    xmlns:view="clr-namespace:Flora.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Orders />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VoucherVM}">
        <view:Vouchers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AddOrderVM}">
        <view:AddOrder />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DetailOrderVM}">
        <view:DetailOrder />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AddVoucherVM}">
        <view:AddVoucher />
    </DataTemplate>
            
    <DataTemplate DataType="{x:Type vm:PlantProductVM}">
        <view:PlantProduct />
    </DataTemplate>

</ResourceDictionary>