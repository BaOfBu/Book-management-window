<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<add name="MyConnectionString" connectionString="Data Source=localhost\TBIN; Trusted_Connection=Yes; Initial Catalog=MyShop;TrustServerCertificate=True" providerName="System.Data.SqlClient"/>
	</connectionStrings>
	<appSettings>
		<add key="entropy" value=""/>
		<add key="password" value=""/>
		<add key="username" value=""/>
		<add key="server" value=""/>
		<add key="database" value=""/>
		<add key="lastWindow" value=""/>
	</appSettings>
	<system.diagnostics>
		<sources>
			<source name="System.Windows.Data" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="xml"/>
				</listeners>
			</source>
		</sources>
		<switches>
			<add name="SourceSwitch" value="Information"/>
		</switches>
		<sharedListeners>
			<add name="xml" type="System.Diagnostics.XmlWriterTraceListener" initializeData="WpfTraceOutput.e2e"/>
		</sharedListeners>
	</system.diagnostics>

</configuration>